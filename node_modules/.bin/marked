#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tantaman/workspace/tantaman.github.io/node_modules/.pnpm/marked@4.0.12/node_modules/marked/bin/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules/.pnpm/marked@4.0.12/node_modules/marked/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules/.pnpm/marked@4.0.12/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules/.pnpm/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules:/Users/tantaman/workspace/node_modules:/Users/tantaman/node_modules:/Users/node_modules:/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules/marked/bin/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules/marked/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/tantaman/workspace/tantaman.github.io/node_modules/.pnpm/marked@4.0.12/node_modules/marked/bin/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules/.pnpm/marked@4.0.12/node_modules/marked/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules/.pnpm/marked@4.0.12/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules/.pnpm/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules:/Users/tantaman/workspace/node_modules:/Users/tantaman/node_modules:/Users/node_modules:/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules/marked/bin/node_modules:/Users/tantaman/workspace/tantaman.github.io/node_modules/marked/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../marked/bin/marked.js" "$@"
else
  exec node  "$basedir/../marked/bin/marked.js" "$@"
fi
